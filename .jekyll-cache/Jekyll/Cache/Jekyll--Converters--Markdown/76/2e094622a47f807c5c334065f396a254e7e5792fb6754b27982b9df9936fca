</pre></td><td class="rouge-code"><pre><span class="kt">Given</span> <span class="n">an</span> <span class="n">array</span> <span class="n">of</span> <span class="n">integers</span><span class="p">,</span> <span class="k">return</span> <span class="n">indices</span> <span class="n">of</span> <span class="n">the</span> <span class="n">two</span> <span class="n">numbers</span> <span class="n">such</span> <span class="n">that</span> <span class="n">they</span> <span class="n">add</span> <span class="n">up</span> <span class="n">to</span> <span class="n">a</span> <span class="n">specific</span> <span class="n">target</span><span class="o">.</span>

<span class="kt">You</span> <span class="n">may</span> <span class="n">assume</span> <span class="n">that</span> <span class="n">each</span> <span class="n">input</span> <span class="n">would</span> <span class="n">have</span> <span class="n">exactly</span> <span class="n">one</span> <span class="n">solution</span><span class="p">,</span> <span class="n">and</span> <span class="n">you</span> <span class="n">may</span> <span class="n">not</span> <span class="n">use</span> <span class="n">the</span> <span class="n">same</span> <span class="n">element</span> <span class="n">twice</span><span class="o">.</span>

<span class="kt">Example</span><span class="p">:</span>

<span class="kt">Given</span> <span class="n">nums</span> <span class="o">=</span> <span class="p">[</span><span class="mi">2</span><span class="p">,</span> <span class="mi">7</span><span class="p">,</span> <span class="mi">11</span><span class="p">,</span> <span class="mi">15</span><span class="p">],</span> <span class="n">target</span> <span class="o">=</span> <span class="mi">9</span><span class="p">,</span>

<span class="kt">Because</span> <span class="n">nums</span><span class="p">[</span><span class="mi">0</span><span class="p">]</span> <span class="o">+</span> <span class="n">nums</span><span class="p">[</span><span class="mi">1</span><span class="p">]</span> <span class="o">=</span> <span class="mi">2</span> <span class="o">+</span> <span class="mi">7</span> <span class="o">=</span> <span class="mi">9</span><span class="p">,</span>
<span class="k">return</span> <span class="p">[</span><span class="mi">0</span><span class="p">,</span> <span class="mi">1</span><span class="p">]</span><span class="o">.</span>
</pre></td></tr></tbody></table></code></pre></div></div>

