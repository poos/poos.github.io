a. ç¬¬ä¸€æ­¥å»ºç«‹.sh æ–‡ä»¶  å¦‚ unusedImage.sh

#!/bin/sh  
PROJ=`find . -name '*.swift' -name '*.xib' -o -name '*.[mh]'`  

for png in `find . -name '*.png'`  
do  
    name=`basename $png`
    if ! grep -qhs "$name" "$PROJ"; then  
        echo "$png is not referenced"  
    fi  
done


